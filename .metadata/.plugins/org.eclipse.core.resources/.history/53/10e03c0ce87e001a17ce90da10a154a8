   package binary_tree_3;

	 class Node{
			char data;
			Node left ,right;
			public Node(char item) {
				this.data=item;
				left=right=null;
			}}
		
public class Bt_by_2_transversal {

	Node root;
	Bt_by_2_transversal(){
		root =null;
		
	}


	public static void main(String[] args) {
