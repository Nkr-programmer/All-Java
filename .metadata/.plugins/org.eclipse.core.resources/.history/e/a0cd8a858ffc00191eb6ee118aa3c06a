package gfgbasic;

import java.util.Scanner;

public class SPrint_k_smallest_elements_in_their_original_order {

	public static void main(String[] args) {
		
		
		
		
		
		
		
		
		  int t;
		   
	    	Scanner sc = new Scanner(System.in);
		    t = sc.nextInt();
		    
		 while(t>0){
				 
				int n,l,k=0,h=0,g=Integer.MAX_VALUE;
				boolean x=false;
				
				n=sc.nextInt();
				
				l=sc.nextInt();
				int a[]=new int [n];
				int b[]=new int [l];
				for(int i=0;i<n;i++)
				{
					
					a[i]=sc.nextInt();
				  
				
				}
			h=g;
				for(int i=0;i<l;i++)
				{
					for(int j=0;j<n;j++)
					{
					if(g>a[j])
					{
						
						b[i]=j;
						g=a[j];
						a[j]=h;
					
						
					}
				  
					}
				}

				for(int j=0;j<l;j++)
				{
				System.out.print(b[j]);

				}
				System.out.println();
				
				
			
				t--;
		 }
		
		
		
		

	}

}
